<launch>

  <include
    file="$(find gazebo_ros)/launch/empty_world.launch" />
  <node
    name="tf_footprint_base"
    pkg="tf"
    type="static_transform_publisher"
    args="0 0 0 0 0 0 base_link base_footprint 40" />
  <node
    name="spawn_model"
    pkg="gazebo_ros"
    type="spawn_model"
    args="-param robot_description -urdf -model BASE_ROBOT"
    output="screen" />
  <rosparam 
    command="load" 
    file="$(find robot_description)/config/control.yaml" />
  <node 
    name="base_controller_spawner" 
    pkg="controller_manager" 
    type="spawner" 
    args="BASE_ROBOT_joint_publisher BASE_ROBOT_velocity_controller"/>
</launch>
